<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Formulario</title>

    <environment include="Production">
        <!-- [if lt IE 9] -->
        <script src="~/assets/js/html5shiv.min.js"></script>
        <script src="~/assets/js/respond.min.js"></script>
        <!-- [endif] -->
        <link href="~/css/EstiloDispensa.min.css" rel="stylesheet" />

        <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet" />

        <meta name="description" content="Material Style Theme">
        <link rel="stylesheet" href="~/assets/css/preload.min.css">
        <link rel="stylesheet" href="~/assets/css/plugins.min.css">
        <link href="~/assets/css/style.grey-800.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/assets/css/width-boxed.min.css" id="ms-boxed" disabled="">
    </environment>

    <environment include="Development">
        <!-- [if lt IE 9] -->
        <script src="~/assets/js/html5shiv.min.js"></script>
        <script src="~/assets/js/respond.min.js"></script>
        <!-- [endif] -->

        <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet" />

        <meta name="description" content="Material Style Theme">
        <link rel="stylesheet" href="~/assets/css/preload.min.css">
        <link rel="stylesheet" href="~/assets/css/plugins.min.css">
        <link href="~/assets/css/style.grey-800.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/assets/css/width-boxed.min.css" id="ms-boxed" disabled="">
    </environment>

</head>
<body>
    <header class="ms-header ms-header-white">
        <!--ms-header-primary-->
        <div class="container container-full">
            <div class="ms-title">
                <a href="/Home/Index">
                    <span class="ms-logo animated zoomInDown animation-delay-5"><i class="far fa-money-bill-alt"></i></span>
                    <h1 class="animated fadeInRight animation-delay-6">Invest<span>M&D</span></h1>
                </a>
            </div>

            <div class="header-right">
                <a data-toggle="modal" data-target="#myModal"
                   class="btn-circle btn-circle-primary no-focus animated zoomInDown animation-delay-8 zmdi-hc-lg">
                    <i class="fas fa-user fa-1x"></i>
                </a>
            </div>
        </div>

        <partial name="_LoginPartial" />


    </header>

    <nav class="navbar navbar-expand-md navbar-static ms-navbar ms-navbar-dark">
        <div class="container container-full">
            <div class="navbar-header">
                <a class="navbar-brand" href="index.html">
                    <!-- <img src="assets/img/demo/logo-navbar.png" alt=""> -->
                    <span class="ms-logo ms-logo-sm">M</span>
                    <span class="ms-title">Material <strong>Style</strong></span>
                </a>
            </div>
            <div class="collapse navbar-collapse" id="ms-navbar">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a asp-controller="Home"
                           asp-action="index" class="nav-link dropdown-toggle animated fadeIn animation-delay-7">Página Inicial</a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link dropdown-toggle animated fadeIn animation-delay-7">Relatório</a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link dropdown-toggle animated fadeIn animation-delay-7">Meus Dados</a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link dropdown-toggle animated fadeIn animation-delay-7">Enviar Conta</a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link dropdown-toggle animated fadeIn animation-delay-7">Dúvidas</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container-fluid">
            <hr />
            <div class="text-center">
                <div class="card card-dark-inverse">
                    <div class="card-body">
                        Contato: contatoInvestMD@gmail.com
                        <br />
                        &copy; 2020 - InvestMD
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <div class="modal modal-dark" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog animated zoomIn animated-3x" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title color-white" id="myModalLabel">Acesso ao sistema</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"><i class="zmdi zmdi-close"></i></span></button>
                </div>
                <div class="modal-body">
                    <div class="row form-group is-empty">
                        <label for="inputUser" class="col-md-2 control-label">Usuário</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="inputUser" placeholder="Usuário">
                        </div>
                    </div>
                    <div class="row form-group is-empty">
                        <label for="inputPassword" class="col-md-2 control-label">Senha</label>
                        <div class="col-md-9">
                            <input type="password" class="form-control" id="inputPassword" placeholder="Senha">
                        </div>
                    </div>
                    <div class="justify-content-between row m-2">
                        <div class="checkbox">
                            <label>
                                <input type="checkbox">
                                <span class="checkbox-material">
                                    <span class="ml-2">Manter-se Conectado</span>
                                </span>
                            </label>
                        </div>
                        <div>
                            <a href="#">Esqueceu-se do seu acesso?</a>
                        </div>
                    </div>
                </div>
                <div class="modal-footer justify-content-between">
                    <button type="button" class="btn btn-raised btn-danger" data-dismiss="modal">Cancelar</button>
                    <a asp-controller="Conta"
                       asp-action="Criar" class="btn btn-raised btn-info">Registrar-se</a>
                    <button type="button" class="btn btn-raised btn-success">Acessar</button>
                </div>
            </div>
        </div>
    </div>
</body>


<!-- Modal -->


</html>
<environment include="Production">
    <script src="~/assets/js/JavaScriptGeral.js"></script>
    <script src="~/assets/js/plugins.min.js"></script>
    <script src="~/assets/js/app.min.js"></script>

</environment>
<environment include="Development">
    <script src="~/assets/js/JavaScriptGeral.js"></script>
    <script src="~/assets/js/plugins.min.js"></script>
    <script src="~/assets/js/app.min.js"></script>
</environment>
@RenderSection("Scripts", required: false)
